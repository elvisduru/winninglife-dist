                  <li role="presentation" class="active">
                    <a href="#<%= event._id%>" aria-controls="music" role="tab" data-toggle="tab">
                      <h5 class="event-date"><%= event.dayFrom %><%= event.dayTo === "same" ? null : " - " + event.dayTo %>
