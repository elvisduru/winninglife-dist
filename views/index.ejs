            <div class="tab-content event-content">
              <% events.forEach((event, index) => { %>
                <% if (index == 0) { %>
                  <div role="tabpanel" class="tab-pane active" id="<%= event._id %>">
                    <div class="event-box">
                      <img src="<%= event.image %>" alt="event" />
                    </div>
                  </div>
                <% } else { %>
                  <div role="tabpanel" class="tab-pane" id="<%= event._id %>">
